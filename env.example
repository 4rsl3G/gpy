PORT=3000
GOBIZ_API_BASE=https://api.gobiz.co.id

# Admin API path tersembunyi (contoh)
ADMIN_PATH=/_pansamin
ADMIN_USER=admin
ADMIN_PASS=supersecret123

# Optional: batasi IP akses (comma separated). kosong = allow all
IP_ALLOWLIST=

# MySQL
DB_HOST=127.0.0.1
DB_PORT=3306
DB_NAME=gobiz_proxy
DB_USER=gobiz_user
DB_PASS=STRONG_PASSWORD

# Sequelize logging
DB_LOGGING=false

# Encryption keys (WAJIB)
# kid aktif:
TOKEN_ENC_KID=v1
# 32 bytes hex => 64 chars (generate pakai: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
TOKEN_ENC_KEY_V1=PASTE_64_HEX_CHARS_HERE
